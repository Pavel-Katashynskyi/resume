<section>
  {{#each reviewList}}
    <div class='card mt-5'>
      <div class='card-body'>
        {{#with title}}
          <div class='card'>
            <div class='card-body'>
              <h5>{{userName}}</h5>
              <div>{{caption}}</div>
              <div class='mt-3'>{{date}}</div>
            </div>
          </div>
        {{/with}}
        {{#with reviewBody}}
          <div class='card'>
            <div class='card=body ms-3'>
              <div>{{seller.title}}: {{seller.value}}</div>
              <div class='mt-3'>{{rating.title}}:
                {{rating.value}}</div>
              <div class='mt-3'>{{text}}</div>
              <div class='mt-3'><strong
                >{{advantages.title}}:</strong></div>
              <div>{{advantages.text}}</div>
              <div class='mt-3'><strong
                >{{disadvantages.title}}:</strong></div>
              <div>{{disadvantages.text}}</div>
              <div class='mt-3 mb-2'>
                {{#each images}}
                  <img src='{{this}}' alt='' />
                {{/each}}
              </div>
            </div>
          </div>
          {{#each coments}}
            <div class='card ms-5 my-2'>
              <div class='card-body'></div>
              <h5 class='ms-2 mb-4'>
                {{userName}}
              </h5>
              <div class='ms-2 mb-3'>
                {{date}}
              </div>
              <div class='ms-2 mb-3'>
                {{text}}
              </div>
            </div>
          {{/each}}
        {{/with}}
      </div>
    </div>
  {{/each}}
</section>